#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/node_modules/.pnpm/source-map-explorer@2.5.3/node_modules/source-map-explorer/bin/node_modules:/opt/node_modules/.pnpm/source-map-explorer@2.5.3/node_modules/source-map-explorer/node_modules:/opt/node_modules/.pnpm/source-map-explorer@2.5.3/node_modules:/opt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/opt/node_modules/.pnpm/source-map-explorer@2.5.3/node_modules/source-map-explorer/bin/node_modules:/opt/node_modules/.pnpm/source-map-explorer@2.5.3/node_modules/source-map-explorer/node_modules:/opt/node_modules/.pnpm/source-map-explorer@2.5.3/node_modules:/opt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../source-map-explorer/bin/cli.js" "$@"
else
  exec node  "$basedir/../source-map-explorer/bin/cli.js" "$@"
fi
